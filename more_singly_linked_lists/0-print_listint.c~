#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

/**
 * main - check the code
 *
 * Return: Always 0.
 */

size_t listint_len(const listint_t *h)
{
int p = 0;
while (h != NULL)
{
if (h == NULL)
{
printf("[o] (nil)\n");
p++;
h = h->next;
}
printf("%d", p);
p++;
h = h->next;
}
return(p);
}
